{
  "manifest_version": 3,
  "name": "E Learning",
  "version": "1.0.0",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "deafult_title": "E Learning"
  },
  "permissions": ["tabs"]
}